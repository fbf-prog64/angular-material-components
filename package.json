    "@angular/compiler-cli": "^21.1.3",
    "@types/jasmine": "~5.1.8",
    "@types/jasminewd2": "~2.0.13",
    "@types/node": "^24.1.0",
    "angular-cli-ghpages": "^2.0.3",
    "codecov": "^3.8.3",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~5.9.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.4",
