    "ngx-mat-datetime-picker",
    "ngx-material-datetime-picker",
    "ngx-mat-timepicker"
  ],
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build -c=production",
    "build:gh-pages": "ng build -c=production --base-href https://fbf-prog64.github.io/angular-material-components/",
    "deploy:gh-pages": "npm run build:gh-pages && npx angular-cli-ghpages --dir=dist/angular-material-components/browser",
    "prettier:fix": "prettier --write projects/**/*.ts",
    "prepare": "npm run prettier:fix",
    "build:datetime-picker": "ng build NgxMatDatetimePicker",
    "build:color-picker": "ng build NgxMatColorPicker",
    "build:file-input": "ng build NgxMatFileInput",
    "publish:datetime-picker": "ng build NgxMatDatetimePicker && cd dist/@ngx-mce/datetime-picker && npm publish --access public && cd .. && cd .. && cd ..",
    "publish:color-picker": "ng build NgxMatColorPicker && cd dist/@ngx-mce/color-picker && npm publish --access public  && cd .. && cd .. && cd ..",
    "publish:file-input": "ng build NgxMatFileInput && cd dist/@ngx-mce/file-input && npm publish --access public  && cd .. && cd .. && cd ..",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "codecov": "codecov",
    "build:ci": "ng build -c=production --output-path docs --base-href https://fbf-prog64.github.io/angular-material-components/"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^21.1.3",
